<phpunit colors="true">
    <logging>
        <log type="coverage-html" target="report" lowUpperBound="35" highLowerBound="70"/>
    </logging>
    <filter>
        <whitelist processUncoveredFilesFromWhitelist="true">
            <directory suffix=".php">app/Control</directory>
            <directory suffix=".php">app/Model</directory>
            <exclude>
                <directory suffix="Test.php">app/Contro</directory>
                <directory suffix="Test.php">app/Model</directory>
            </exclude>
        </whitelist>
    </filter>
</phpunit>